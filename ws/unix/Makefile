%.o : $(SOURCEPATH)/%.cpp
	$(CXX)  $(CXXOPTS) $(DBG_EXT) $(OPTI_EXT) $(DEBUG) $(MEM_DEBUG) $(INCLUDEPATH_GENETIC) $(INCLUDEPATH) -c $< -o $@

%.o : $(SOURCEPATH_GENETIC)/%.cpp
	$(CXX)  $(CXXOPTS) $(DBG_EXT) $(OPTI_EXT) $(DEBUG) $(MEM_DEBUG) $(INCLUDEPATH_GENETIC) $(INCLUDEPATH) -c $< -o $@

default:
	make all

help:
	# make all        : builds genetic main program
	# make clean      : cleans genetic main program
	# --------------------------------------------------
	# make debug      : set debug mode of build params
	# make release    : set release mode of build params
	# --------------------------------------------------
	# make linux      : set linux as target system
	# make irix       : set irix n32 as target system
	# make arm        : set arm linux as target system
	# make irix_gcc   : set irix gcc as target system
	# make osx        : set Mac OS X as target system
	# --------------------------------------------------
	# ext.. _skipwchar
	#	_nowchar
	#	_rtti
	#	_skipwchar_rtti
	#	_nowchar_rtti
	# --------------------------------------------------
	# ext2.._64 for 64 bit system
	# --------------------------------------------------


$(ENAME): $(FILES)  $(GEN_FILES) $(DEPLIBS)
	test -d $(EDIR) || mkdir -p $(EDIR)
	$(CXX)  $(DBG_EXT) $(LINKOPTS) -o $(ENAME) $(FILES) $(GEN_FILES) $(LIBS) $(EXT_LIBS)
	$(DEPUPDATE)
	# ------------------ done making Genetic SW ---------------- 

sinclude depend


all:
	make $(ENAME)
	# -------------------- done complete rebuild ----------------------

run:
	make
	./$(ENAME)
	
	
clean:
	touch xx.d
	rm -f *.d
	rm -f depend
	rm -f core
	rm -f so_locations
	rm -rf ii_files
	rm -rf $(FILES)
	rm -rf $(GEN_FILES)
	rm -rf $(NAME)
	# ----------------- done cleaning up -------------------------------

debug:
	echo "DBG_EXT= -g" > buildflags
	echo "DEBUG =-DGZ_DEBUG" >> buildflags
	echo "#OPTI_EXT= -O2 $(OPTFLAGS)" >> buildflags
	echo "#MEM_DEBUG =-DGZ_MEM_DEBUG" >> buildflags
	echo "#DP_FLAG=-DGZ_DOUBLE_PRECISION" >> build-flags
	echo "#DP_EXT=-dp" >> buildflags
	make clean
	# ----------------- done debug setup --------------------------

release:
	echo "#DBG_EXT= -g" > buildflags
	echo "#DEBUG =-DGZ_DEBUG" >> buildflags
	echo "OPTI_EXT= -O2 $(OPTFLAGS)" >> buildflags
	echo "#MEM_DEBUG =-DGZ_MEM_DEBUG" >> buildflags
	echo "#DP_FLAG=-DGZ_DOUBLE_PRECISION" >> buildflags
	echo "#DP_EXT=-dp" >> buildflags
	make clean
	# ----------------- done release setup --------------------------

irix_gcc:
	echo "LINKOPTS = " > compilerflags
	echo "CC = gcc" >> compilerflags
	echo "AR = ar" >> compilerflags
	echo "CCOPTS =  -MD -DGZ_SGI  " >> compilerflags
	echo "CXXOPTS =  -MD -DGZ_SGI  " >> compilerflags
	echo "DEPUPDATE = cat *.d > depend" >> compilerflags
	echo "OPTFLAGS = " >> compilerflags
	echo "EXT_LIBS = -lpthread -ldl -lm" >> compilerflags
	echo "SHARED = -shared" >> compilerflags
	echo "RANLIB = echo" >> compilerflags
	echo "SO_EXT = .so" >> compilerflags
	echo "PLATFORM = irix_gcc" >> compilerflags
	echo "EDIR = ." >> compilerflags
	# ----------------- done irix GCC setup --------------------------

irix:
	echo "LINKOPTS = -n32 -L/usr/lib32/internal " > compilerflags
	echo "AR = ar" >> compilerflags
	echo "CXX = CC" >> compilerflags
	echo "CCOPTS =  -DGZ_SGI -MDupdate depend -n32 -woff 1209,1682,3180,1314 " >> compilerflags
	echo "CXXOPTS =  -DGZ_SGI -MDupdate depend -n32 -woff 1209,1682,3180,1314 " >> compilerflags
	echo "DEPUPDATE = " >> compilerflags
	echo "OPTFLAGS = -OPT:Olimit=15000 " >> compilerflags
	echo "EXT_LIBS = -lpthread -ldl -lm" >> compilerflags
	echo "SHARED = -shared" >> compilerflags
	echo "RANLIB = echo" >> compilerflags
	echo "SO_EXT = .so" >> compilerflags
	echo "PLATFORM = irix" >> compilerflags
	echo "EDIR = ." >> compilerflags
	# ----------------- done irix setup --------------------------


