	echo "AR = ar" >> compilerflags
	echo "CCOPTS =  -m32 -Wall -Wextra -MD -DGZ_MAC -pipe -fshort-wchar -fPIC  -I/" >> compilerflags
	echo "CXXOPTS = -m32 -Wall -Wextra -MD -DGZ_MAC -pipe -fshort-wchar -fno-rtti -fPIC  -I/" >> compilerflags
	echo "CXX = c++" >> compilerflags
	echo "DEPUPDATE = cat *.d > depend" >> compilerflags
	echo "OPTFLAGS = " >> compilerflags
	echo "EXT_LIBS =   -framework Carbon" >> compilerflags
	echo "RANLIB = ranlib" >> compilerflags
	echo "SHARED = -dynamiclib" >> compilerflags
	echo "SO_EXT = .dylib" >> compilerflags
	echo "PLATFORM = osx" >> compilerflags
	echo "EDIR = $(NAME)/Contents/MacOS" >> compilerflags
	echo "SUDO = sudo" >> compilerflags
	# ----------------- done Mac OSX setup --------------------------

osx_64:
	echo "LINKOPTS = -m64" > compilerflags
	echo "AR = ar" >> compilerflags
	echo "CCOPTS =  -m64 -Wall -Wextra -MD -DGZ_MAC -pipe -fshort-wchar -fPIC  -I/" >> compilerflags
	echo "CXXOPTS = -m64 -Wall -Wextra -MD -DGZ_MAC -pipe -fshort-wchar -fno-rtti -fPIC  -I/" >> compilerflags
	echo "CXX = c++" >> compilerflags
	echo "DEPUPDATE = cat *.d > depend" >> compilerflags
	echo "OPTFLAGS = " >> compilerflags
	echo "EXT_LIBS =   -framework Carbon" >> compilerflags
	echo "RANLIB = ranlib" >> compilerflags
	echo "SHARED = -dynamiclib" >> compilerflags
	echo "SO_EXT = .dylib" >> compilerflags
	echo "PLATFORM = osx_64" >> compilerflags
	echo "EDIR = $(NAME)/Contents/MacOS" >> compilerflags
	echo "SUDO = sudo" >> compilerflags
	echo "PLATFORM_BIT = 64" >> compilerflags
	# ----------------- done Mac OSX setup --------------------------

ver:
	# Platform:$(PLATFORM) Version:$(VERSION) Text:$(VERSION_TEXT)

